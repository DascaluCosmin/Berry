<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="380.0" prefWidth="748.0" style="-fx-background-color: linear-gradient(#DAE2F8, #916698);" stylesheets="/css/accountUser.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controller.AccountUserController">
   <children>
      <TableView fx:id="tableViewAccountUser" layoutX="374.0" layoutY="14.0" prefHeight="283.0" prefWidth="350.0" stylesheets="@../css/accountUser.css">
        <columns>
            <TableColumn prefWidth="75.0" text="Your friends">
               <columns>
                <TableColumn fx:id="tableColumnFirstName" prefWidth="164.99999618530273" text="First name" />
                <TableColumn fx:id="tableColumnLastName" minWidth="0.0" prefWidth="188.59996032714844" text="Last name" />
               </columns>
            </TableColumn>
        </columns>
      </TableView>
      <Button fx:id="buttonAddFriendship" layoutX="377.0" layoutY="313.0" mnemonicParsing="false" onMouseClicked="#addFriendshipRequest" prefHeight="46.0" prefWidth="132.0" stylesheets="@../css/accountUser.css" text="Add friendship request" />
      <Button fx:id="buttonDeleteFriendship" layoutX="591.0" layoutY="313.0" mnemonicParsing="false" onMouseClicked="#deleteFriendship" prefHeight="46.0" prefWidth="132.0" stylesheets="@../css/accountUser.css" text="Delete friendship" />
      <Label fx:id="labelUserName" alignment="CENTER" layoutX="13.0" layoutY="14.0" prefHeight="46.0" prefWidth="333.0" stylesheets="@../css/accountUser.css" text="UserName">
         <font>
            <Font size="22.0" />
         </font></Label>
      <Button fx:id="buttonChangeProfilePhoto" layoutX="13.0" layoutY="313.0" mnemonicParsing="false" onMouseClicked="#changeProfilePhotoEvent" prefHeight="46.0" prefWidth="132.0" stylesheets="@../css/accountUser.css" text="Change Profile Photo" />
      <ImageView fx:id="profilePhotoImageView" fitHeight="225.0" fitWidth="225.0" layoutX="67.0" layoutY="73.0" pickOnBounds="true">
         <image>
            <Image url="@../images/noProfilePhoto.png" />
         </image></ImageView>
   </children>
</AnchorPane>
