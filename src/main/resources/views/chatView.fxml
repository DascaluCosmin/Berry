<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="502.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controller.ChatViewController">
   <children>
      <ListView fx:id="listViewConversation" layoutX="-2.0" layoutY="-2.0" prefHeight="416.0" prefWidth="622.0" />
      <Button fx:id="buttonSend" layoutX="516.0" layoutY="461.0" mnemonicParsing="false" onMouseClicked="#sendMessage" prefHeight="26.0" prefWidth="103.0" text="Send" />
      <TextField fx:id="textFieldMessage" layoutX="471.0" layoutY="427.0" promptText="Message" />
      <Label fx:id="labelUsername" layoutX="14.0" layoutY="418.0" prefHeight="26.0" prefWidth="150.0" text="Hello, username!" />
      <Button fx:id="buttonInterrupt" layoutX="309.0" layoutY="432.0" mnemonicParsing="false" text="Interrupt" onMouseClicked="#interruptEvent"/>
   </children>
</AnchorPane>
